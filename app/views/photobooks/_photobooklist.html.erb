<div class="photobook">
  <ul>
    <% photobook.each do |photob| %>
    <li>
      <a href="#"  class="showphoto" data-photobookid="<%=photob.id%>">
        <div class="bookcover">
          <div class="pic">
            <%=image_tag  photob.bookcover? ? photob.bookcover : photob.photos.first.file_url(:thumb) %>
          </div>
        </div>
        <div class="bookname"><%= photob.bookname%></div>
      </a>
    </li>
  <% end %>
    <div class="justify_fix"></div>
  </ul>
</div>