<%= image_tag(current_user.avatar_url(:big)) if current_user.avatar%>
<%= form_for(@user,url:{action:"avatar"},html:{multipart:true,method:"post"}) do |f|%>
<div class="field">
  <%= f.file_field :avatar%>
  <%= f.hidden_field :avatar_cache %>
  <%= f.submit "上传头像", class: "btn btn-primary btn-sm" %>
      </div>
  <% end %>
